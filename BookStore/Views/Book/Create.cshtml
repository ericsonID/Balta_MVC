@model BookStore.ViewModels.EditorBookViewModel
@{
    ViewBag.Title = "Criar um novo Livro";
}
@helper Minhafuncao(string nome)
{
    <text>Voce está logado como: @nome</text>
}
<p>
    @Minhafuncao("Ericson Cordeiro de Lima")
</p>
<h2>Novo Livro</h2>
@using (Html.BeginForm())
{

    @Html.Partial("Partials/EditorBookPartial", Model)
    <button type="submit" class="btn btn-default">Criar</button>

}

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}
